<h1><%= title %></h1>
<%- messages() %> <% if (errors ){ %>
<ul class="notice">
  <% errors.array().forEach(error => { %>
  <li><%= error.msg %></li>
  <% }) %>
</ul>
<% } %>
<div class="account-management">
  <h2>Welcome <%= accountData.account_firstname %></h2>
  <a
    class="accountInfo"
    href="/account/account-info/<%=  accountData.account_id %>"
    >Update Account Information</a
  >
  <% if (managementLink){ %>
  <div class="management"><%- managementLink %></div>
  <% } %>
</div>
